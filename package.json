{
  "name": "counter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch",
    "lint:test": "eslint './test/**/*(*.jsx|*.js)'",
    "lint": "eslint './src/**/*(*.jsx|*.js)'",
    "build-css": "node-sass --output-style compressed --include-path node_modules/bourbon/app/assets/stylesheets src/scss/main.scss ./dist/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "watch-js": "watchify --extension=.jsx -v --debug ./src/index.jsx -t babelify --outfile ./dist/app.js",
    "build-js": "browserify --extension=.jsx ./src/index.jsx -t babelify | uglifyjs -mc > ./dist/app.js",
    "dev": "npm-run-all --parallel lint watch-js watch-css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "bourbon": "^4.2.7",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "classnames": "^2.2.5",
    "eslint": "^2.5.3",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-standard": "^1.3.2",
    "jsdom": "^8.2.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.7.0",
    "nodemon": "^1.9.2",
    "npm-run-all": "^1.7.0",
    "reactify": "^1.1.1",
    "uglify-js": "^2.6.2",
    "watchify": "^3.7.0",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "es6-promise": "^3.1.2",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "react": "^0.14.8",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.5",
    "redux": "^3.3.1"
  }
}
